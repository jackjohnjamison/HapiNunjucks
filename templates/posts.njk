{% extends "furniture.njk" %}

{% block main %} 
   <h1>Hello bitches</h1>

    <p>{{model.message}}</p>

    <section> 
        {% for post in model.posts %}
            <h3>{{post.title}}</h3>
            <p>{{post.body}}</p>
        {% endfor %}    
    </section>
 
{% endblock %}

{% block scripts %}
    {% if debug %}
        <script> const modelDebug = {{debug | safe}} </script>
    {% endif %}
{% endblock %}

